mI <- numeric(0)
for(i in seq_along(det)){
N <- det[[i]]$N
I <- det[[i]]$I
if(i > 1){
if(length(N) < ncol(mN)){
N <- c(N, numeric(ncol(mN) - length(N)))
I <- c(I, numeric(ncol(mI) - length(I)))
} else if (length(N) > ncol(mN)){
m <- matrix(0, nrow = nrow(mN), ncol = length(N) - ncol(mN))
mN <- cbind(mN, m)
mI <- cbind(mI, m)
} else {
F
}
}
mN <- rbind(mN, N)
mI <- rbind(mI, I)
}
